{
    "applications": {
        "gecko": {
            //"id": "{d886e695-bf02-46e7-95e7-bb7ec45ed725}",
            "id": "chris@temp2.tld",
            "strict_min_version": "100.0"
        }
    },
    "background": {
        "scripts": [
            "background-MV2.js"
        ]
    },
    "options_ui": {
        "page": "popup.html",
        "open_in_tab": false
    },
    "browser_action": {
        "browser_style": true,
        "default_icon": "icons/icon_32.png",
        "default_popup": "popup.html"
    },
    "commands": {
        "toggle-interruption": {
            "suggested_key": {
                "default": "Ctrl+Shift+U",
                "mac": "Command+Shift+U"
            },
            "description": "Toggle interruption"
        }
    },
    "description": "uget integration with fixed annoyances for personal use.",
    "homepage_url": "http://127.0.0.1",
    "icons": {
        "32": "icons/icon_32.png",
        "48": "icons/icon_48.png",
        "64": "icons/icon_64.png",
        "128": "icons/icon_128.png"
    },
    "manifest_version": 2,
    "minimum_chrome_version": "100.0",
    "name": "uGet Integration (personal) MV2",
    "short_name": "uGet",
    "permissions": [
        "*://*/*",
        "webRequestBlocking",
        "nativeMessaging",
        "contextMenus",
        "webRequest",
        "downloads",
        "activeTab",
        "storage",
        "cookies",
        "tabs"
    ],
    "version": "0.0.3.46"
